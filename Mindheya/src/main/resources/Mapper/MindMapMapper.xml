<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.my.map.Dao.MindMapMapper">

  	<select id="selectShare" resultType="Member" parameterType="String">
  	SELECT 
  		ID,
  		EMAIL
  	FROM
  		MEMBER
  	WHERE
  		id=#{id}
  	</select>
  	
  
  
  </mapper>